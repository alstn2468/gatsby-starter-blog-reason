{
  "name": "gatsby-starter-blog-reason",
  "gentypeconfig": {
    "language": "typescript",
    "module": "es6",
    "importPath": "relative",
    "generatedFileExtension": ".gen.tsx",
    "debug": {
      "all": true,
      "basic": false
    }
  },
  "reason": { "react-jsx": 3 },
  "bs-dependencies": [
    "bs-webapi",
    "@rescript/react",
    "@reasonml-community/graphql-ppx"
  ],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    },
    {
      "module": "commonjs",
      "in-source": false
    }
  ],
  "suffix": ".bs.js",
  "refmt": 3,
  "ppx-flags": [
    [
      "@reasonml-community/graphql-ppx/ppx",
      "-template-tag=graphql",
      "-fragment-in-query=exclude"
    ]
  ]
}
