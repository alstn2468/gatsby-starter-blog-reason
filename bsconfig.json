{
  "name": "gatsby-starter-blog-reason",
  "gentypeconfig": {
    "language": "typescript",
    "module": "es6",
    "importPath": "relative",
    "generatedFileExtension": ".gen.tsx",
    "debug": {
      "all": true,
      "basic": false
    }
  },
  "reason": { "react-jsx": 3 },
  "bs-dependencies": [
    "@rescript/react"
  ],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    },
    {
      "module": "commonjs",
      "in-source": false
    }
  ],
  "suffix": ".bs.js",
  "refmt": 3
}
