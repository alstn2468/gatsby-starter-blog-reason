  "scripts": {
    "prepare-gatsby": "babel gatsby -d . -x .ts,.tsx --presets gatsby-package",
    "postinstall": "yarn prepare-gatsby",
    "build": "yarn prepare-gatsby && yarn re:build && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "re:build": "bsb -make-world -clean-world",
    "re:watch": "bsb -make-world -clean-world -w",
    "stylelint": "stylelint --fix 'src/**/*.{ts,tsx}'"
  },
  "repository": {
    "type": "git",
