  "scripts": {
    "graphql:codegen": "graphql codegen",
    "prepare-gatsby": "babel gatsby -d . -x .ts,.tsx --presets gatsby-package",
    "postinstall": "yarn prepare-gatsby",
    "build": "yarn prepare-gatsby && yarn re:build && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean && rescript clean",
    "dev": "yarn prepare-gatsby && yarn re:build && gatsby develop",
    "re:build": "rescript",
    "re:start": "rescript build -w",
    "re:format": "rescript format -all",
    "stylelint": "stylelint --fix 'src/**/*.{ts,tsx}'",
    "lint": "eslint --ext .ts,.tsx gatsby src --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --ext .ts,.tsx --fix gatsby src --no-error-on-unmatched-pattern"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alstn2468/gatsby-starter-blog-reason.git"
  },
  "keywords": [
